<h2>Movies</h2>
<ul>
    {{#each movies}}
    <li>
        <a href="/movies/{{_id}}">{{title}}</a>
    </li>

    <section class="celeb">
        <a href="/movies/{{_id}}/edit">Edit</a>
      <form action="/movies/{{_id}}/delete" method="POST">
        <button>Delete</button>
      </form>
    </section>

    {{/each}}
</ul>

<a href="/movies/new">Add Movie</a>
